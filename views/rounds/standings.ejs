<%- include('../partials/header') %>  
<ul class="subnav nav nav-pills nav-justified">
  <li class="nav-item">
    <a class="nav-link text-success" href="/events">Rounds</a>
  </li>
  <li class="nav-item">
    <a class="nav-link bg-success active" href="/standings">Standings</a>
  </li>
</ul>

<table class="table text-center">
  
  <tr>
    <% valuesArray[0].forEach(v => { %>
      <th><%= v %></th>
    <% }) %>
  </tr>

  <% for (let i = 1; i < valuesArray.length; i++) { %>
    <tr>
      <% valuesArray[i].forEach(v => { %>
        <td><%= v %></td>
      <% }) %>
    </tr>
  <% } %>


</table>



</table>